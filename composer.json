{
  "name": "nazg/skeleton",
  "description": "Http Microframework for HHVM/Hack",
  "type": "project",
  "keywords": [
    "hhvm",
    "hack",
    "psr",
    "psr-7",
    "psr-11",
    "psr-15",
    "framework"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=7.0",
    "hhvm": ">=3.23",
    "hhvm/hhvm-autoload": "^1.5",
    "nazg/framework": "^0.2.0",
    "facebook/xhp-lib": "~2.2",
    "zendframework/zend-diactoros": "^1.6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "91carriage/phpunit-hhi": "^5.7",
    "ytake/gardening-hhvm": "^0.1.0",
    "facebook/fbexpect": "^0.4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.x-dev"
    }
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  }
}
