{
  "name": "nazg/skeleton",
  "description": "Http Microframework for Hack",
  "keywords": [
    "hhvm",
    "hack",
    "psr",
    "psr-7",
    "psr-11",
    "psr-15"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Yuuki Takezawa",
      "email": "yuuki.takezawa@comnect.jp.net"
    }
  ],
  "require": {
    "php": ">=7.0",
    "hhvm": ">=3.20.0",
    "hhvm/hhvm-autoload": "^1.5",
    "nazg/framework": "0.1.0-alpha1",
    "facebook/xhp-lib": "~2.2",
    "zendframework/zend-diactoros": "^1.6.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "91carriage/phpunit-hhi": "^6.2.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "minimum-stability": "stable"
}
